{
  "name": "money-pit-api",
  "version": "1.0.0",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "boilerplate",
    "eslint"
  ],
  "author": "Sagi Hillel",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "postinstall": "chmod +x ./scripts/dev.sh",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "nodemon -L",
    "ts-node-server": "ts-node src/server.ts",
    "start": "set LOCAL=true && nodemon --watch ./**/*.ts --exec ts-node src/server.ts",
    "clean-docker": "docker-compose rm -f && docker image prune -a -f",
    "fresh-docker": "npm run clean-docker && docker-compose up",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "migration": "bash ./scripts/migration.sh",
    "heroku-deploy": "heroku container:push web --app money-pit-api && heroku container:release web --app money-pit-api"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "1.20.0",
    "compression": "1.7.4",
    "cron-parser": "^4.4.0",
    "crypto": "^1.0.1",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "joi": "17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "newrelic": "^8.15.0",
    "node-xlsx": "^0.21.0",
    "puppeteer": "^14.1.1",
    "swagger-ui-express": "4.3.0",
    "uuid": "^8.3.2",
    "uuid-by-string": "^3.0.7",
    "winston": "3.7.2",
    "winston-transport": "4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "1.7.2",
    "@types/cryptr": "^4.0.1",
    "@types/express": "4.17.13",
    "@types/hapi__joi": "17.1.8",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.21",
    "@types/puppeteer": "^5.4.6",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "27.5.1",
    "jest-express": "1.12.0",
    "nodemon": "2.0.15",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "license": "MIT"
}
